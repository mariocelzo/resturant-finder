{
  "expo": {
    "name": "NearBite",
    "slug": "near-bite",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/NearBiteLogo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "resturant-finder",
    "splash": {
      "image": "./assets/NearBiteLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FDE4B2"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nearbite.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/NearBiteLogo.png",
        "backgroundColor": "#FDE4B2"
      },
      "package": "com.nearbite.app",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/NearBiteLogo.png"
    }
  }
}
