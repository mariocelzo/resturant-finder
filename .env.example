# Supabase Configuration
# Ottieni le tue credenziali da: https://app.supabase.com/project/_/settings/api
EXPO_PUBLIC_SUPABASE_URL=your_supabase_project_url_here
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Google Places API
# Ottieni la tua API key da: https://console.cloud.google.com/apis/credentials
# Assicurati di abilitare: Places API, Maps JavaScript API, Geocoding API
EXPO_PUBLIC_GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# Backend Proxy Railway (CONSIGLIATO per proteggere le API keys)
# Opzione 1: Usa il backend Railway deployato (API keys protette sul server)
EXPO_PUBLIC_USE_BACKEND_PROXY=true
EXPO_PUBLIC_BACKEND_URL=https://nearbite-backend-production.up.railway.app

# Opzione 2: Usa backend locale per development
# EXPO_PUBLIC_USE_BACKEND_PROXY=true
# EXPO_PUBLIC_BACKEND_URL=http://localhost:3000

# Opzione 3: Chiama direttamente le API (sconsigliato per repo pubbliche)
# EXPO_PUBLIC_USE_BACKEND_PROXY=false
